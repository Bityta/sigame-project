{
  "title": "üéØ Game Service",
  "uid": "game-service",
  "tags": ["sigame", "game", "backend"],
  "timezone": "browser",
  "refresh": "10s",
  "time": {
    "from": "now-15m",
    "to": "now"
  },
  "editable": true,
  "graphTooltip": 1,
  "panels": [
    {
      "id": 1,
      "title": "‚úÖ Success Requests (2xx)",
      "type": "timeseries",
      "gridPos": {"h": 8, "w": 8, "x": 0, "y": 0},
      "targets": [
        {
          "expr": "sum(rate(http_requests_total{job=\"game-service\",status=~\"2..\"}[1m])) by (endpoint)",
          "legendFormat": "{{endpoint}}",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": {"mode": "palette-classic"},
          "custom": {
            "axisPlacement": "auto",
            "fillOpacity": 20,
            "gradientMode": "opacity",
            "lineWidth": 2,
            "showPoints": "never"
          },
          "unit": "reqps"
        }
      },
      "options": {
        "legend": {"displayMode": "list", "placement": "bottom"},
        "tooltip": {"mode": "multi", "sort": "desc"}
      }
    },
    {
      "id": 2,
      "title": "‚ö†Ô∏è Client Errors (4xx)",
      "type": "timeseries",
      "gridPos": {"h": 8, "w": 8, "x": 8, "y": 0},
      "targets": [
        {
          "expr": "sum(rate(http_requests_total{job=\"game-service\",status=~\"4..\"}[1m])) by (endpoint)",
          "legendFormat": "{{endpoint}}",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": {"mode": "palette-classic"},
          "custom": {
            "axisPlacement": "auto",
            "fillOpacity": 30,
            "gradientMode": "opacity",
            "lineWidth": 2,
            "showPoints": "auto"
          },
          "unit": "reqps"
        },
        "overrides": [
          {
            "matcher": {"id": "byName"},
            "properties": [
              {"id": "color", "value": {"mode": "fixed", "fixedColor": "yellow"}}
            ]
          }
        ]
      },
      "options": {
        "legend": {"displayMode": "list", "placement": "bottom"},
        "tooltip": {"mode": "multi", "sort": "desc"}
      }
    },
    {
      "id": 3,
      "title": "üî• Server Errors (5xx)",
      "type": "timeseries",
      "gridPos": {"h": 8, "w": 8, "x": 16, "y": 0},
      "targets": [
        {
          "expr": "sum(rate(http_requests_total{job=\"game-service\",status=~\"5..\"}[1m])) by (endpoint)",
          "legendFormat": "{{endpoint}}",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": {"mode": "fixed", "fixedColor": "red"},
          "custom": {
            "axisPlacement": "auto",
            "fillOpacity": 40,
            "gradientMode": "opacity",
            "lineWidth": 3,
            "showPoints": "always"
          },
          "unit": "reqps"
        }
      },
      "options": {
        "legend": {"displayMode": "list", "placement": "bottom"},
        "tooltip": {"mode": "multi", "sort": "desc"}
      }
    },
    {
      "id": 4,
      "title": "üìä Total Request Rate",
      "type": "timeseries",
      "gridPos": {"h": 8, "w": 12, "x": 0, "y": 8},
      "targets": [
        {
          "expr": "sum(rate(http_requests_total{job=\"game-service\"}[1m]))",
          "legendFormat": "Total RPS",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": {"mode": "palette-classic"},
          "custom": {
            "axisPlacement": "auto",
            "fillOpacity": 25,
            "gradientMode": "hue",
            "lineWidth": 3,
            "showPoints": "never"
          },
          "unit": "reqps",
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {"color": "green", "value": null},
              {"color": "yellow", "value": 10},
              {"color": "red", "value": 50}
            ]
          }
        }
      },
      "options": {
        "legend": {"displayMode": "list", "placement": "bottom"},
        "tooltip": {"mode": "single"}
      }
    },
    {
      "id": 5,
      "title": "‚è±Ô∏è Response Time (p50, p95, p99)",
      "type": "timeseries",
      "gridPos": {"h": 8, "w": 12, "x": 12, "y": 8},
      "targets": [
        {
          "expr": "histogram_quantile(0.50, sum(rate(http_request_duration_seconds_bucket{job=\"game-service\"}[1m])) by (le))",
          "legendFormat": "p50",
          "refId": "A"
        },
        {
          "expr": "histogram_quantile(0.95, sum(rate(http_request_duration_seconds_bucket{job=\"game-service\"}[1m])) by (le))",
          "legendFormat": "p95",
          "refId": "B"
        },
        {
          "expr": "histogram_quantile(0.99, sum(rate(http_request_duration_seconds_bucket{job=\"game-service\"}[1m])) by (le))",
          "legendFormat": "p99",
          "refId": "C"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": {"mode": "palette-classic"},
          "custom": {
            "axisPlacement": "auto",
            "fillOpacity": 15,
            "gradientMode": "none",
            "lineWidth": 2,
            "showPoints": "never"
          },
          "unit": "s",
          "decimals": 3
        }
      },
      "options": {
        "legend": {"displayMode": "table", "placement": "right", "calcs": ["mean", "lastNotNull", "max"]},
        "tooltip": {"mode": "multi", "sort": "none"}
      }
    },
    {
      "id": 6,
      "title": "üéÆ Active Sessions",
      "type": "stat",
      "gridPos": {"h": 6, "w": 6, "x": 0, "y": 16},
      "targets": [
        {
          "expr": "game_sessions_active",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": {"mode": "thresholds"},
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {"color": "blue", "value": null},
              {"color": "green", "value": 1},
              {"color": "yellow", "value": 10},
              {"color": "red", "value": 50}
            ]
          },
          "unit": "short"
        }
      },
      "options": {
        "colorMode": "background",
        "graphMode": "area",
        "orientation": "auto",
        "textMode": "value_and_name",
        "reduceOptions": {"values": false, "calcs": ["lastNotNull"]}
      }
    },
    {
      "id": 7,
      "title": "üìä Total Sessions",
      "type": "stat",
      "gridPos": {"h": 6, "w": 6, "x": 6, "y": 16},
      "targets": [
        {
          "expr": "game_sessions_total",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": {"mode": "thresholds"},
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {"color": "blue", "value": null},
              {"color": "green", "value": 10}
            ]
          },
          "unit": "short"
        }
      },
      "options": {
        "colorMode": "background",
        "graphMode": "area",
        "orientation": "auto",
        "textMode": "value_and_name",
        "reduceOptions": {"values": false, "calcs": ["lastNotNull"]}
      }
    },
    {
      "id": 8,
      "title": "üîå WebSocket Connections",
      "type": "stat",
      "gridPos": {"h": 6, "w": 6, "x": 12, "y": 16},
      "targets": [
        {
          "expr": "game_ws_connections",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": {"mode": "thresholds"},
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {"color": "blue", "value": null},
              {"color": "green", "value": 1},
              {"color": "orange", "value": 50}
            ]
          },
          "unit": "short"
        }
      },
      "options": {
        "colorMode": "background",
        "graphMode": "area",
        "orientation": "auto",
        "textMode": "value_and_name",
        "reduceOptions": {"values": false, "calcs": ["lastNotNull"]}
      }
    },
    {
      "id": 9,
      "title": "‚ùì Questions Answered",
      "type": "stat",
      "gridPos": {"h": 6, "w": 6, "x": 18, "y": 16},
      "targets": [
        {
          "expr": "game_questions_answered_total",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": {"mode": "thresholds"},
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {"color": "blue", "value": null},
              {"color": "green", "value": 10}
            ]
          },
          "unit": "short"
        }
      },
      "options": {
        "colorMode": "background",
        "graphMode": "area",
        "orientation": "auto",
        "textMode": "value_and_name",
        "reduceOptions": {"values": false, "calcs": ["lastNotNull"]}
      }
    },
    {
      "id": 10,
      "title": "‚ö° Button Press Latency (p50, p95, p99)",
      "type": "timeseries",
      "gridPos": {"h": 8, "w": 12, "x": 0, "y": 22},
      "targets": [
        {
          "expr": "histogram_quantile(0.50, sum(rate(game_button_press_latency_seconds_bucket[1m])) by (le))",
          "legendFormat": "p50",
          "refId": "A"
        },
        {
          "expr": "histogram_quantile(0.95, sum(rate(game_button_press_latency_seconds_bucket[1m])) by (le))",
          "legendFormat": "p95",
          "refId": "B"
        },
        {
          "expr": "histogram_quantile(0.99, sum(rate(game_button_press_latency_seconds_bucket[1m])) by (le))",
          "legendFormat": "p99",
          "refId": "C"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": {"mode": "palette-classic"},
          "custom": {
            "axisPlacement": "auto",
            "fillOpacity": 20,
            "gradientMode": "none",
            "lineWidth": 2,
            "showPoints": "never"
          },
          "unit": "s",
          "decimals": 3
        }
      },
      "options": {
        "legend": {"displayMode": "table", "placement": "right", "calcs": ["mean", "lastNotNull", "max"]},
        "tooltip": {"mode": "multi", "sort": "none"}
      }
    },
    {
      "id": 11,
      "title": "üîå gRPC Request Rate",
      "type": "timeseries",
      "gridPos": {"h": 8, "w": 12, "x": 12, "y": 22},
      "targets": [
        {
          "expr": "sum(rate(grpc_requests_total{job=\"game-service\"}[1m])) by (method, status)",
          "legendFormat": "{{method}} ({{status}})",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": {"mode": "palette-classic"},
          "custom": {
            "axisPlacement": "auto",
            "fillOpacity": 20,
            "gradientMode": "opacity",
            "lineWidth": 2,
            "showPoints": "never"
          },
          "unit": "reqps"
        }
      },
      "options": {
        "legend": {"displayMode": "table", "placement": "right"},
        "tooltip": {"mode": "multi", "sort": "desc"}
      }
    }
  ]
}
